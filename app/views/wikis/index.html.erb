<h1>All Wikis</h1>
<%= link_to "Create a new wiki!", new_wiki_path, class: 'btn btn-success' %>
<div class="row">
   <div class="col-md-8">
      <% @wikis.each do |wiki| %>
      <div class = "media">
      <div class="media-body">
        <h4 class="wiki-list">
            <% if wiki.private == true %>
               <%= link_to wiki.title, wiki %>  (Private)
           <% else %>
               <%= link_to wiki.title, wiki %>
             <% end %>
      </h4>
    </div>
  </div>
    <% end %>
  </div>
</div>
